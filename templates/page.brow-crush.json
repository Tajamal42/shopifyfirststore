    "16626168156c19e206": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<style>\n.countdown{\ndisplay: grid;\n    grid-template-columns: 1fr 1fr 1fr 1fr;\n    \n}\n.countdown h3{\n    text-align: center;\n    padding-bottom: 0px;\n    margin-bottom: 10px;\n}\n.countdown h4{\n    color: #FFF;\n    text-align: left;\n    letter-spacing: 50px;\n    font-size: 50px;\n    margin: 0px;\n    line-height: 126px;\n}\n.bg-counter{\nbackground: url(https:\/\/cdn.shopify.com\/s\/files\/1\/0658\/9648\/1023\/files\/countdown-bg.png?v=1662683354);\n    width: 170px;\n    height: 150px;\n    background-repeat: no-repeat;\n    padding-left: 0px;\n    background-size: 150px;\n}\n<\/style>\n<section class=\"coming-soon\"><div class=\"page-width\"><div class=\"countdown\"><div class=\"container-day\"><h3>Days<\/h3>\n<h4 class=\"day bg-counter\"><\/h4>\n <\/div>\n<div class=\"container-hour\">               <h3>Hours<\/h3>\n<h4 class=\"hour bg-counter\"><\/h4>\n<\/div><div class=\"container-minute\">               <h3>Minutes<\/h3>\n<h4 class=\"minute bg-counter\"><\/h4>\n<\/div>\n<div class=\"container-second\">                <h3>Seconds<\/h3>\n<h4 class=\"second bg-counter\"><\/h4>\n<\/div>\n<\/div>\n<\/div>\n<\/section>\n<script>\nconst countdown = () => {\n    const countDate = new Date(\"Sept 23, 2022, 00:00:00\").getTime();\n    const now = new Date().getTime();\n    const gap = countDate - now;\n    \n    \/\/how Time works\n    const second = 1000;\n    const minute = second * 60; \n    const hour = minute * 60;\n    const day = hour * 24;\n\n    \/\/calculate\n    const textDay = Math.floor(gap \/ day);\n    const textHour = Math.floor((gap % day)\/hour);\n    const textMinute = Math.floor((gap % hour)\/minute);\n    const textSecond = Math.floor((gap % minute)\/second);\n    \n    document.querySelector(\".day\").innerText = textDay;\n    document.querySelector(\".hour\").innerText = textHour;\n    document.querySelector(\".minute\").innerText = textMinute;\n    document.querySelector(\".second\").innerText = textSecond;\n};\n\nsetInterval(countdown , 1000);\n<\/script>",
        "color_scheme": "background-1",
        "padding_top": 40,
        "padding_bottom": 52
      }
    }
